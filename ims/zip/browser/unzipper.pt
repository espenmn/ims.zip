<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="plone">

<head>
    <metal:noborder fill-slot="top_slot"
                 tal:define="dummy python:request.set('disable_border',1)" />
</head>

<body>
<div metal:fill-slot="main">
  <fieldset>
    <legend>Import a zip file</legend>
    <form enctype="multipart/form-data" tal:attributes="action string:${here/absolute_url}/@@unzip" method="POST">
      <label for="file">File</label><br/>
      <input type="file" name="file"/><br/><br/>
      <input type="hidden" name="force_files:boolean" tal:attributes="value request/force_files|string:False" />

      <input type="hidden" name="form.submitted" value="1"/>
      <input type="submit" value="Submit" class="context"/>
    </form>
  </fieldset>
</div>
</body>
</html>